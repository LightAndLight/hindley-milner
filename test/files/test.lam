class Eq a where {
  eq : a -> a -> Bool
    }

not : Bool -> Bool
not b = case b of {
  true -> false;
  _ -> true
  }

instance Eq Bool where {
  eq a b = case a of {
    true -> b;
    false -> not b
    }
}

asdf a = eq a a

test = asdf true

and a b = case a of {
  true -> b;
  _ -> false
  }

data List a = Nil | Cons a (List a)

instance Eq a => Eq (List a) where {
  eq = rec eq' la lb = case la of {
    Nil -> case lb of {
      Nil -> true;
      _ -> false
    };
    Cons a rest -> case lb of {
      Cons b rest' -> and (eq a b) (eq' rest rest');
      _ -> false
    }
  } in eq'
}

test2 = asdf (Cons true Nil)
